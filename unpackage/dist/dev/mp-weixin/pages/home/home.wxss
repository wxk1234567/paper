
.searchBar{
  height: 54px;
}
.swiperItem{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
}
.scanIcon{
  width: 50px;
  height: 54px;
  position: absolute;
  top: 0px;
  right: 0px;
  z-index: 99;
}
.goodCards{
  width: 100%;
  height: 200px;
}
.goodText{
  position: relative;
  display: flex;
  flex-direction: column;
}
.money{
  font-weight: bold;
  font-size: 1.2rem;
}
.addIcon{
  position: absolute;
  bottom: 0;
  right: 0;
  width: 30px;
  height: 30px;
  border-top-left-radius: 18px;
  background-color: black;
}

